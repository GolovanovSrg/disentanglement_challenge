{
    "model": {"encoder": {"embedding_size": 16,
                          "kernel_size": 32,
                          "backbone_type": "resnet18"},
              "autoregressor": {"channel": 256,
                                "kernel_size": 3,
                                "n_block": 4,
                                "n_res_block": 1,
                                "res_channel": 128,
                                "attention": true,
                                "n_head": 8,
                                "dropout": 0},
              "predictor": {"reccurent": false,
                            "n_predictions": 5},
              "pca": {"enable": false,
                      "n_components": 1,
                      "whiten": false,
                      "n_data": 50000}},
    "trainer": {"batch_size": 512,
                "n_epochs": 20,
                "optimizer_params": {"lr": 1e-4,
                                     "weight_decay": 0.001,
                                     "warmup": 200,
                                     "amsgrad": true},
                "checkpoint_path": "checkpoints/best_checkpoint.pt",
                "devices": [2, 3],
                "test_part": 0.2,
                "n_jobs": 12}
}

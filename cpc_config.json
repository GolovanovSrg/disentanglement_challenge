{
    "model": {"encoder": {"embedding_size": 32,
                          "kernel_size": 16,
                          "backbone_type": "resnet34"},
              "autoregressor": {"channel": 128,
                                "kernel_size": 5,
                                "n_block": 2,
                                "n_res_block": 1,
                                "res_channel": 64,
                                "attention": true,
                                "n_head": 4,
                                "dropout": 0.1},
              "predictor": {"reccurent": false,
                            "n_predictions": 5},
              "pca": {"enable": true,
                      "n_components": 0.9,
                      "whiten": true,
                      "n_data": 50000}},
    "trainer": {"batch_size": 128,
                "n_epochs": 10,
                "optimizer_params": {"lr": 1e-3,
                                     "weight_decay": 0,
                                     "amsgrad": true},
                "device": "cuda:0",
                "n_jobs": 8}
}
